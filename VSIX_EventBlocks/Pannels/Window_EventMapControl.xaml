<UserControl x:Class="VSIX_EventBlocks.Window_EventMapControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:vsshell="clr-namespace:Microsoft.VisualStudio.Shell;assembly=Microsoft.VisualStudio.Shell.15.0"
             Background="{DynamicResource {x:Static vsshell:VsBrushes.WindowKey}}"
             Foreground="{DynamicResource {x:Static vsshell:VsBrushes.WindowTextKey}}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             Name="MyToolWindow">
    <Grid>
        <StackPanel Orientation="Vertical" Background="#FF2D2D30">
            <TextBlock Margin="10,10,177,10" HorizontalAlignment="Center" Foreground="White" Height="18" Width="113"><Run Text="Block Logic"/><Run Text=" Analyser"/><LineBreak/><Run/><LineBreak/><Run/></TextBlock>
        </StackPanel>
    </Grid>
</UserControl>
